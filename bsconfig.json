{
  "name": "reason-scripts",
  "generate-merlin": true,
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "bs-dependencies": [
    "reason-react",
    "@glennsl/bs-jest",
    "@jsiebern/bs-material-ui",
    "reason-apollo",
    "vrroom",
    "@glennsl/rebase",
    "@glennsl/bs-json"
  ],
  "reason": {
    "react-jsx": 2
  },
  "bsc-flags": ["-bs-super-errors"],
  "refmt": 3,
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "ppx-flags": [
    "./node_modules/@jsiebern/bs-material-ui/ppx_withStyles",
    "graphql_ppx/ppx"
  ],
  "suffix": ".bs.js"
}
